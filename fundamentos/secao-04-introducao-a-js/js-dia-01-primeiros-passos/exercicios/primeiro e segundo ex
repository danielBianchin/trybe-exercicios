const a = 5;
const b = 10;
const c = 15;

console.log(a + b)
console.log(a - b)
console.log(a * b)
console.log(a / b)
console.log (a % b)

if (a > b) {
    console.log("O maior número é o:", a);
} else {
    console.log("O maior número é o:", b);
}

if (a > b && a > c) {
    console.log("O maior número é o:", a)
} else if (b > a && b > c) {
    console.log("O maior número é o:", b)
} else {
    console.log("O maior número é o:", c)
}

